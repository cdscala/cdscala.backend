
<h1>Carrito: {{carrito}}</h1>
<div class="table-container">
    <div class="inner">
        <table>
            <thead>
                <tr>
                <th>id</th>
                <th>title</th>
                <th>description</th>
                <th>price</th>
                <th>thumbnail</th>
                <th>code</th>
                <th>stock</th>
                <th>status</th>
                <th>quantity</th>
                </tr>
            </thead>
            <tbody>
            {{#each products as |product|}}
                <tr>
                    <td>{{product.product._id}}</td>
                    <td>{{product.product.title}}</td>
                    <td>{{product.product.description}}</td>
                    <td>{{product.product.price}}</td>
                    <td>{{product.product.thumbnail}}</td>
                    <td>{{product.product.code}}</td>
                    <td>{{product.product.stock}}</td>
                    <td>{{product.product.status}}</td>
                    <td>{{product.quantity}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
    <div class="table__caption">
        <h2 class="table__header">Lista de productos</h2>
        <p class="table__header--sub"></p>
    </div>
</div> 

